{
    "canisters": {
        "challenge": {
            "type": "custom",
            "main": "src/canisters/challenge.ts",
            "candid": "src/dids/challenge.did",
            "build": "npx azle challenge",
            "wasm": ".azle/challenge/challenge.wasm",
            "gzip": true,
            "declarations": {
                "output": "src/front/declarations/challenge"
            }
        },
        "token": {
            "type": "custom",
            "main": "src/canisters/token.ts",
            "candid": "src/dids/token.did",
            "build": "npx azle token",
            "wasm": ".azle/token/token.wasm",
            "gzip": true,
            "declarations": {
                "output": "src/front/declarations/token"
            }
        },
        "front": {
            "type": "assets",
            "candid": "src/dids/front.did",
            "main": "src/front/index.html",
            "build": "npx azle front",
            "front": {
                "entrypoint": "src/front/index.html"
            },
            "source": ["src/front/public", "src/front/build", "src/front/dist"]
        },
        "icrc1_ledger_canister": {
            "type": "custom",
            "candid": "https://raw.githubusercontent.com/dfinity/ic/d87954601e4b22972899e9957e800406a0a6b929/rs/rosetta-api/icrc1/ledger/ledger.did",
            "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ic-icrc1-ledger.wasm.gz",
            "declarations": {
                "output": "src/frontend/declarations/icrc1_ledger_canister"
            }
        },
        "internet_identity": {
            "type": "custom",
            "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
            "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
            "remote": {
                "candid": "internet_identity.did",
                "id": {
                    "ic": "bnz7o-iuaaa-aaaaa-qaaaa-cai"
                }
            },
            "declarations": {
                "output": "src/frontend/declarations/internet_identity"
            }
        }
    },
    "defaults": {
        "replica": {
            "subnet_type": "system"
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:4349"
        }
    }
}
